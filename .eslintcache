 '../../organisms/AsideMenu';\nimport { HOME_WORDS } from '../../../core/router/paths';\nimport WordsCard from '../../molecules/WordsCard';\n\nconst Home: React.FunctionComponent = () => {\n  const history = useHistory();\n  const dispatch = useDispatch();\n\n  const checkCurrentUser = useCallback(async () => {\n    database.auth().onAuthStateChanged((user) => {\n      if (!user) {\n        history.push('/login');\n      } else {\n        const auth = database.auth();\n        const { currentUser } = auth;\n        const { uid, displayName, email } = (currentUser as unknown) as Record<\n          string,\n          string\n        >;\n\n        dispatch(\n          setUser({\n            user: {\n              uid,\n              displayName,\n              email,\n            },\n          })\n        );\n      }\n    });\n  }, [history, dispatch]);\n\n  useEffect(() => {\n    checkCurrentUser();\n  }, [checkCurrentUser]);\n\n  return (\n    <Base>\n      <AsideMenu />\n      <Switch>\n        <Route path={HOME_WORDS}>\n          <Cards />\n        </Route>\n      </Switch>\n    </Base>\n  );\n};\n\nexport default Home;\n","/Users/zhiganov/Documents/Projects/memorize_words/src/core/router/paths.ts",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/core/store/index.ts",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/core/store/rootReducer.ts",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/reportWebVitals.ts",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/core/store/models/translate.ts",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/core/store/api/translate.ts",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/index.tsx",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/components/organisms/AsideMenu/index.tsx",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/components/molecules/WordsCard/index.tsx",["105","106","107","108"],"/* eslint-disable jsx-a11y/media-has-caption */\nimport React, { memo, useEffect, useCallback, useState, useMemo } from 'react';\nimport dictionaryApi from 'core/store/api/dictionary';\nimport VolumeUpIcon from '@material-ui/icons/VolumeUp';\nimport styles from './styles.module.css';\nimport VideosList from '../VideosList';\n\ntype Props = {\n  word: string;\n  translate: string;\n};\n\nconst getExamples = (data: any) => {\n  const [wordData] = data;\n  const { meanings, phonetics: phoneticsList } = wordData;\n  const [phonetics] = phoneticsList;\n  const { audio } = phonetics;\n\n  const result = meanings.reduce((acc: any, item: any) => {\n    const { definitions } = item;\n    definitions.forEach(({ example }: any) => acc.push(example));\n    return acc;\n  }, []);\n\n  return {\n    examples: result,\n    audio,\n  };\n};\n\nconst WordsCard: React.FunctionComponent<Props> = ({\n  word,\n  translate,\n}: Props) => {\n  const [examples, setExamples] = useState<string[]>([]);\n  const [audioSrc, setAudioSrc] = useState<string>('');\n  const getDefinition = useCallback(async () => {\n    const data = await dictionaryApi.getDefinition(word);\n\n    if (data && Array.isArray(data)) {\n      const { examples: resExamples, audio: resAudio } = getExamples(data);\n      setExamples(resExamples);\n      setAudioSrc(resAudio);\n    }\n  }, [word]);\n\n  useEffect(() => {\n    if (word) {\n      getDefinition();\n    }\n  }, [word, getDefinition]);\n\n  const audio = useMemo(() => {\n    return new Audio(audioSrc);\n  }, [audioSrc]);\n\n  const handlePlayAudio = useCallback(() => {\n    audio.play();\n  }, [audio]);\n\n  return (\n    <div className={styles.container}>\n      <div className={styles.wordSection}>\n        <div>\n          <span>{`${word}-${translate}`}</span>\n        </div>\n\n        <div className={styles.audioButtonContainer}>\n          <button\n            className={styles.audioButton}\n            type=\"button\"\n            onClick={handlePlayAudio}\n          >\n            <VolumeUpIcon />\n          </button>\n        </div>\n      </div>\n      <div className={styles.contextSection}>\n        <ul className={styles.examplesList}>\n          {Array.isArray(examples) && examples.length\n            ? examples\n                .filter((def) => def)\n                .map((def) => (\n                  <li className={styles.examplesItem}>\n                    <span className={styles.example} key={def}>\n                      {def}\n                    </span>\n                  </li>\n                ))\n            : null}\n        </ul>\n      </div>\n      <div className={styles.videoSection}>\n        {word ? <VideosList keyword={word} /> : null}\n      </div>\n    </div>\n  );\n};\n\nexport default memo(WordsCard);\n","/Users/zhiganov/Documents/Projects/memorize_words/src/core/store/api/dictionary.ts",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/components/molecules/VideosList/index.tsx",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/core/store/api/videos.ts",[],"/Users/zhiganov/Documents/Projects/memorize_words/src/components/molecules/VideosList/VideoItem/index.tsx",[],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":10,"column":8,"nodeType":"125","messageId":"126","endLine":10,"endColumn":17},{"ruleId":"127","severity":1,"message":"128","line":13,"column":28,"nodeType":"129","messageId":"130","endLine":13,"endColumn":31,"suggestions":"131"},{"ruleId":"127","severity":1,"message":"128","line":19,"column":40,"nodeType":"129","messageId":"130","endLine":19,"endColumn":43,"suggestions":"132"},{"ruleId":"127","severity":1,"message":"128","line":19,"column":51,"nodeType":"129","messageId":"130","endLine":19,"endColumn":54,"suggestions":"133"},{"ruleId":"127","severity":1,"message":"128","line":21,"colum