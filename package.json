      "~c/(.*)": [
        "<rootDir>/src/components/$1"
      ],
      "~hooks/(.*)": [
        "<rootDir>/src/utils/hooks/$1"
      ],
      "~actions/(.*)": [
        "<rootDir>/src/core/store/models/$1"
      ],
